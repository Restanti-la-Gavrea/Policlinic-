<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="policlinica.controllers.AngajatiListController">
   <children>
      <ScrollPane fitToHeight="true" fitToWidth="true" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" scaleZ="2.0" VBox.vgrow="ALWAYS">
         <content>
            <TableView fx:id="angajatiTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" scaleZ="2.0">
              <columns>
                <TableColumn fx:id="nrContractCol" minWidth="100.0" prefWidth="-1.0" text="Numar Contract" />
                <TableColumn fx:id="numeCol" minWidth="100.0" prefWidth="-1.0" text="Nume" />
                  <TableColumn fx:id="prenumeCol" minWidth="100.0" prefWidth="-1.0" text="Prenume" />
                  <TableColumn fx:id="postCol" minWidth="100.0" prefWidth="-1.0" text="Post" />
                  <TableColumn fx:id="nrTelefonCol" minWidth="100.0" prefWidth="-1.0" text="Numar Telefon" />
              </columns>
            </TableView>
         </content>
      </ScrollPane>
      <Button fx:id="seeDetailsBtn" mnemonicParsing="false" styleClass="blueButton" text="Vezi Detalii" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
