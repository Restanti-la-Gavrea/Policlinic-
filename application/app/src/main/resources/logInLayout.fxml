<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox fx:id="pane" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="330.0" minWidth="300.0" spacing="5.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="policlinica.controllers.LogInController">
   <children>
      <Label fx:id="errLogInLbl" text="Username or Password did not match!" VBox.vgrow="NEVER">
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding></Label>
      <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="30.0" spacing="10.0" VBox.vgrow="NEVER">
         <children>
            <Label text="username" />
            <TextField fx:id="usernameField" />
         </children>
      </HBox>
      <HBox alignment="CENTER" maxWidth="1.7976931348623157E308" prefHeight="30.0" spacing="10.0" VBox.vgrow="NEVER">
         <children>
            <Label text="password">
               <HBox.margin>
                  <Insets right="1.0" />
               </HBox.margin>
            </Label>
            <PasswordField fx:id="passwordField" />
         </children>
      </HBox>
      <Button fx:id="logInBtn" mnemonicParsing="false" onAction="#logIn" prefWidth="149.0" text="Log In">
         <VBox.margin>
            <Insets left="61.0" top="3.0" />
         </VBox.margin>
      </Button>
      <Button fx:id="testBtn" layoutX="266.0" layoutY="234.0" mnemonicParsing="false" onAction="#test" prefWidth="149.0" text="Test">
         <VBox.margin>
            <Insets left="61.0" top="3.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
