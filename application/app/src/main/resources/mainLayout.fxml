                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </Button>
            <Button fx:id="programariBtn" mnemonicParsing="false" onAction="#setProgramariLayout" prefHeight="37.0" prefWidth="100.0" styleClass="whiteText" text="Programari">
