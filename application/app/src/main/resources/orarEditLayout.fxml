<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" styleClass="whiteBackground" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="policlinica.controllers.OrarEditController">
   <children>
      <Button fx:id="backButton" mnemonicParsing="false" onAction="#goBack" text="&lt; Back">
         <VBox.margin>
            <Insets bottom="10.0" left="1.0" right="10.0" top="10.0" />
         </VBox.margin>
      </Button>
      <VBox fx:id="saptamanalBox" alignment="CENTER" spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="53.0" text="Luni: " />
                  <TextField fx:id="luniFld" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="53.0" text="Marti: " />
                  <TextField fx:id="martiFld" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="53.0" text="Miercuri: " />
                  <TextField fx:id="miercuriFld" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="53.0" text="Joi: " />
                  <TextField fx:id="joiFld" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="53.0" text="Vineri: " />
                  <TextField fx:id="vineriFld" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="53.0" text="Sambata:" />
                  <TextField fx:id="sambataFld" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="53.0" text="Duminica:" />
                  <TextField fx:id="duminicaFld" prefWidth="200.0" />
               </children>
            </HBox>
            <Button fx:id="saptamanalSubmitBtn" mnemonicParsing="false" onAction="#submitSaptamanal" styleClass="blueButton" text="Modifica Orar Generic (Saptamanal)">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <VBox fx:id="specificBox" alignment="CENTER" spacing="10.0" VBox.vgrow="ALWAYS">
         <children>
            <Label fx:id="specificDayLbl" text="Label" />
            <TextField fx:id="specificFld" />
            <Button fx:id="specificSubmitBtn" mnemonicParsing="false" onAction="#submitSpecific" styleClass="blueButton" text="Modifica Orar Specific (Ziua)">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
         </children></VBox>
      <VBox fx:id="concediuBox" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" spacing="5.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="53.0" text="Prima Zi:" />
                  <DatePicker fx:id="firstDayFld" prefWidth="200.0" />
               </children>
            </HBox>
            <HBox alignment="CENTER" spacing="5.0">
               <children>
                  <Label prefHeight="25.0" prefWidth="53.0" text="Ultima Zi:" />
                  <DatePicker fx:id="lastDayFld" prefWidth="200.0" />
               </children>
            </HBox>
            <Button fx:id="concediuSubmitBtn" mnemonicParsing="false" onAction="#submitConcediu" styleClass="blueButton" text="Modifica Concediu">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin></Button>
         </children></VBox>
   </children>
   <padding>
      <Insets bottom="200.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
